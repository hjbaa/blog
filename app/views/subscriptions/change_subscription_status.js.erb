if (document.getElementById('subscribe-link').innerText === 'Follow') {
    document.getElementById('subscribe-link').className = 'btn btn-outline-primary'
    document.getElementById('subscribe-link').innerText = 'Unfollow'
} else {
    document.getElementById('subscribe-link').className = 'btn btn-primary'
    document.getElementById('subscribe-link').innerText = 'Follow'
}